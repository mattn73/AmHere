{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^0.6.0",
        "slim/http": "^0.8.0",
        "php-di/php-di": "^6.0",
        "selective/config": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
